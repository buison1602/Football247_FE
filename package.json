{
  "name": "football247_fe",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "dev:clean": "powershell -ExecutionPolicy Bypass -File dev-start.ps1",
    "dev:recovery": "powershell -ExecutionPolicy Bypass -File dev-recovery.ps1",
    "dev:safe": "taskkill /F /IM node.exe & rmdir /s /q .next & npm cache clean --force & next dev",
    "build": "next build",
    "start": "next start",
    "clean": "rmdir /s /q .next & rmdir /s /q node_modules & npm install"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.2.0",
    "@mui/material": "^7.2.0",
    "axios": "^1.10.0",
    "next": "^15.3.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "24.0.13",
    "typescript": "5.8.3"
  }
}
